{
  "name": "feature-docker-containers-ui",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/features/docker-containers/ui/src",
  "prefix": "containerized",
  "tags": ["type:libs", "scope:ui", "feature:docker-containers"],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/features/docker-containers/ui/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/features/docker-containers/ui/**/*.ts",
          "libs/features/docker-containers/ui/**/*.html"
        ]
      }
    },
    "i18n:extract": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "cwd": "libs/features/docker-containers/ui",
        "command": "ngx-translate-extract --input ./src --output ./src/assets/i18n/{en,fr}.json --sort --clean --format namespaced-json --format-indentation '  '"
      }
    }
  }
}
